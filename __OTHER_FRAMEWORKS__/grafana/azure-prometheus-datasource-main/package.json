    "@swc/jest": "0.2.39",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint": "9.6.1",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "24.2.1",
